    "titleBar.activeBackground": "$COLOUR1_",
    "titleBar.inactiveBackground": "$COLOUR0_",
    "widget.shadow": "$COLOUR0_00"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "$COLOUR4_" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "$COLOURB_" }
    },
    {
      "name": "String Regex",
      "scope": "string.regexp",
      "settings": { "foreground": "$COLOUR9_" }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": { "foreground": "$COLOURC_" }
    },
    {
      "name": "Booleans",
      "scope": "constant.language",
      "settings": { "foreground": "$COLOUR9_" }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": { "foreground": "$COLOURE_" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "$COLOURD_" }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": { "foreground": "$COLOURA_" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.parameter"],
      "settings": { "foreground": "$COLOUR5_" }
    },
    {
      "name": "Constants",
      "scope": ["constant", "support.constant"],
      "settings": { "foreground": "$COLOUR6_" }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": { "foreground": "$COLOURD_" }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": { "foreground": "$COLOUR5_" }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": { "foreground": "$COLOUR8_" }
    },
    {
      "name": "Invalid Deprecated",
      "scope": "invalid.deprecated",
      "settings": { "foreground": "$COLOUR9_" }
    },
    {
      "name": "Markdown Headings",
      "scope": "markup.heading",
      "settings": { "foreground": "$COLOURE_", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Links",
      "scope": "markup.underline.link",
      "settings": { "foreground": "$COLOURD_" }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic" }
    }
  ]
}
